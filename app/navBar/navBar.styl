.navbar
  border-bottom 1px solid #CDD3D8
  font-weight 400
  font-family 'Raleway', Arial, Helvetica, Sans-serif
  background-color rgba(0,0,0,0.05)
  box-shadow 0px 3px 5px rgba(0,0,0,0.1)

  .nabbar-list
    max-width 1200px
    position relative
    margin-left auto
    margin-right auto
    list-style none
    li
      line-height 48px
      display: inline
      margin-right 20px
      a
        color: #949494
        cursor pointer
        text-decoration none
      a:hover
        color: #75AAD8
        text-decoration none